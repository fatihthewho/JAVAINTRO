public class prime {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        System.out.println("enter number");
        int number=input.nextInt();
        int divider=0;
        for(int i=2; i<number;i++)
        {
            if(number % i ==0)
            {
            divider++;
            }
        }
        
        if(divider==0)
            System.out.println("prime");
        else
            System.out.println("not prime");

    }
